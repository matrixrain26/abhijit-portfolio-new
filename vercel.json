{
  "version": 2,
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "outputDirectory": ".next",
  "regions": ["bom1"],
  "github": {
    "silent": true,
    "autoAlias": true,
    "enabled": true,
    "branch": "master"
  },
  "env": {
